<Window x:Class="Quiz2.Question5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quiz2"
        mc:Ignorable="d"
        Title="Question4" Height="450" Width="800">
    <Grid>
        <TreeView>
            <TreeViewItem Header="Even" x:Name="evenNumber"></TreeViewItem>
            <TreeViewItem Header="Odd" x:Name="oddNumber"></TreeViewItem>
        </TreeView>
        <Button x:Name="btn" HorizontalAlignment="Center" Click="btn_Click">Click on me</Button>
    </Grid>
</Window>
